#include <iostream>
using namespace std;
void factorial(){
    unsigned int n;
    unsigned long long factorial = 1;
    cin >> n;
    for(int i = 1; i <=n; ++i)
    {
        factorial *= i;
    }
    cout << "Factorial of " << n << " = " << factorial;    
    }
int main(){
    cout<<"Input a number: ";
    factorial();
return 0;}
